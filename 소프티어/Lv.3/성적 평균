#include<iostream>
#include<vector>
#include<cstdio>
using namespace std;

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int N, K; 
    cin >> N >> K;

    vector<int> v;
    for(int i = 0; i < N; i++) {
        int score;
        cin >> score;
        v.push_back(score);
    }

    float avg;
    for(int j = 0; j < K; j++) {
        avg = 0.0;
        int a, b;
        cin >> a >> b;
        for(int n = a - 1; n < b; n++) {
            avg += v[n];
        }
        printf("%.2f\n", (avg / (b - a + 1)));
    }
    return 0;
}
